{
  "name": "@lanistasoftware/lui-vue",
  "version": "0.1.11",
  "private": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "sb": "start-storybook -p 6006",
    "sb-build": "build-storybook",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.2.23"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@storybook/addon-a11y": "^6.4.9",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addons": "^6.4.9",
    "@storybook/theming": "^6.4.9",
    "@storybook/vue3": "^6.4.9",
    "@vitejs/plugin-vue": "^1.10.2",
    "autoprefixer": "^10.3.6",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-storybook": "^0.5.5",
    "eslint-plugin-vue": "^7.18.0",
    "husky": "^7.0.2",
    "hygen": "^6.1.0",
    "pinst": "^2.1.6",
    "postcss": "^8.3.11",
    "prettier": "2.4.1",
    "remixicon": "^2.5.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "storybook-builder-vite": "^0.1.11",
    "tailwind-safelist-generator": "^0.1.3",
    "tailwindcss": "^2.2.16",
    "vite": "^2.7.1",
    "vite-plugin-eslint": "^1.3.0",
    "vue-loader": "^16.8.1"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/lui-vue.umd.js",
  "module": "./dist/lui-vue.es.js",
  "exports": {
    ".": {
      "import": "./dist/lui-vue.es.js",
      "require": "./dist/lui-vue.umd.js"
    }
  }
}
